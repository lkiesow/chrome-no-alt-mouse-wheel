{
  "manifest_version": 2,

  "name": "No-Alt-Mouse-Wheel",
  "description": "Prevent mouse wheel scrolling if alt is pressed",
  "version": "0.2",
  "author": "Lars Kiesow",
  "homepage_url": "https://github.com/lkiesow/chromw-no-alt-mouse-wheel",

  "content_scripts": [
    {
      "all_frames": true,
      "matches": ["*://*/*"],
      "js": ["no-alt-wheel.js"]
    }
  ]
}
